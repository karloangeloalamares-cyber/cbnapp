{
  "expo": {
    "name": "cbn-app",
    "slug": "cbn-app",
    "version": "1.0.0",
    "scheme": "cbnapp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kalamares.cbnapp",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false
        },
        "NSPhotoLibraryUsageDescription": "CBN Ads needs access to your photo library so you can attach images and videos to your News and Announcements posts.",
        "NSCameraUsageDescription": "CBN Ads needs access to your camera so you can take photos to attach to your posts.",
        "NSMicrophoneUsageDescription": "CBN Ads needs access to your microphone to record audio for video posts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.kalamares.cbnapp",
      "googleServicesFile": "./google-services.json",
      "allowBackup": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "60214d42-0c2f-408f-8930-e7bd7b0cf54f"
      }
    },
    "owner": "kalamares",
    "plugins": [
      "expo-web-browser",
      "expo-video",
      "expo-audio",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "networkInspector": false
          },
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableMinifyInReleaseBuilds": true
          }
        }
      ],
      "expo-asset"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/60214d42-0c2f-408f-8930-e7bd7b0cf54f"
    }
  }
}